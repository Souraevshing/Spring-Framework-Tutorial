<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean class="com.spring.demo.beans.Laptop" name="laptop">
    </bean>

    <bean class="com.spring.demo.beans.Desktop" name="desktop">
    </bean>


    <bean class="com.spring.demo.beans.Programmer" id="programmer1">
        <property name="name" value="Sachin"/>
        <property name="computer" ref="laptop"/>
    </bean>

    <bean class="com.spring.demo.beans.Programmer" id="programmer2">
        <property name="name" value="Sachin"/>
        <property name="computer" ref="laptop"/>
    </bean>

</beans>